@model IEnumerable<AutoNuoma.ViewModels.AutoListViewModel>

@{
    ViewBag.Title = "Zaidimai";
}

<h2>Zasalisimai</h2>

<p>
    @Html.ActionLink("Naujas zaidimas", "Create", "Zaidimas", new { @class="btn btn-success"})
</p>
<table class="table table-striped datatable">
    <thead>
    <tr style="background-color: lightblue">
        <th>
            @Html.DisplayNameFor(model => model.valstybinisNr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.busena)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.modelis)
        </th>
        <th style="wsalisth: 18%"></th>
    </tr>
        </thead>
    <tbody>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.valstybinisNr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.busena)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.organizacija) @Html.DisplayFor(modelItem => item.modelis)
        </td>
        <td>
            @Html.ActionLink("Redaguoti", "Edit", new { salis=item.salis }, new { @class= "btn btn-primary action" }) 
            @Html.ActionLink("Šalinti", "Delete", new { salis=item.salis }, new { @class= "btn btn-danger action" })
        </td>
    </tr>
}
        </tbody>

</table>
